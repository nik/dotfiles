" No audible bell
set vb

" No toolbar
set guioptions-=T

" Use console dialogs
set guioptions+=c
"let g:solarized_termcolors=256
"let g:solarized_contrast= 'high'
"let g:solarized_visibility= 'high'
"set background=dark
colorscheme monokai
set guifont=Monaco:h14
set colorcolumn=80
highlight ColorColumn guibg=gray20
